{
    "name": "riskfortress-intelligence",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "format": "prettier --write .",
        "test": "jest --coverage",
        "test:e2e": "playwright test",
        "test:ci": "jest --ci --coverage",
        "security:audit": "npm audit --production",
        "security:scan": "snyk test",
        "prepare": "husky",
        "type-check": "tsc --noEmit",
        "analyze": "ANALYZE=true next build",
        "generate-sitemap": "tsx src/scripts/generate-sitemap.ts",
        "generate-og": "tsx src/scripts/generate-og.ts",
        "build:cloudflare": "node build-cloudflare.js",
        "deploy:cloudflare": "npm run build:cloudflare && wrangler pages deploy .next --project-name=riskfortress",
        "deploy:staging": "NEXT_PUBLIC_APP_URL=https://staging.riskfortress.com npm run build:cloudflare && wrangler pages deploy .next --project-name=riskfortress-staging"
    },
    "dependencies": {
        "@hookform/resolvers": "^3.3.4",
        "@types/react-reconciler": "^0.32.3",
        "@types/webxr": "^0.5.24",
        "@upstash/ratelimit": "2.0.8",
        "@upstash/redis": "^1.36.1",
        "@vercel/analytics": "1.1.1",
        "@vercel/og": "0.6.2",
        "@vercel/speed-insights": "1.0.1",
        "clsx": "2.0.0",
        "critters": "^0.0.23",
        "crypto-js": "4.2.0",
        "cssnano": "^7.1.2",
        "date-fns": "3.3.1",
        "framer-motion": "10.16.16",
        "lucide-react": "0.344.0",
        "next": "^16.1.3",
        "next-seo": "6.5.0",
        "next-sitemap": "4.2.3",
        "next-themes": "0.2.1",
        "react": "19.2.3",
        "react-dom": "19.2.3",
        "react-hook-form": "7.49.3",
        "recharts": "2.10.3",
        "sharp": "0.33.2",
        "sonner": "1.4.1",
        "tailwind-merge": "2.2.0",
        "tailwindcss": "3.4.1",
        "typescript": "5.3.3",
        "zod": "3.22.4"
    },
    "devDependencies": {
        "@playwright/test": "^1.57.0",
        "@svgr/webpack": "8.1.0",
        "@testing-library/jest-dom": "6.1.6",
        "@testing-library/react": "14.1.2",
        "@testing-library/user-event": "14.5.1",
        "@types/node": "20.11.5",
        "@types/react": "19.2.8",
        "@types/react-dom": "19.2.3",
        "@typescript-eslint/eslint-plugin": "6.19.0",
        "@typescript-eslint/parser": "6.19.0",
        "autoprefixer": "10.4.17",
        "eslint": "8.56.0",
        "eslint-config-next": "14.1.0",
        "husky": "9.0.6",
        "jest": "29.7.0",
        "jest-environment-jsdom": "29.7.0",
        "lint-staged": "15.2.0",
        "postcss": "8.4.33",
        "prettier": "3.2.4",
        "snyk": "1.1269.0",
        "ts-jest": "29.1.2",
        "tsx": "4.7.0",
        "webpack-bundle-analyzer": "4.10.1"
    },
    "engines": {
        "node": ">=18.17.0",
        "npm": ">=9.0.0"
    }
}